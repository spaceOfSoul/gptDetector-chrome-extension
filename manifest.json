{
    "manifest_version": 3,
    "name": "gptDetector",
    "description": "인공지능이 생성한 텍스트인지 판별하여 줍니다.",
    "version": "1.0",
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content_script.js"]
    }]
}